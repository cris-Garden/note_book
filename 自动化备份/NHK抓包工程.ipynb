    "            movieurl = str(dayNew[\"news_easy_movie_uri\"])\n",
    "            #newsImage,\n",
    "            newimage = str(dayNew[\"news_web_image_uri\"])\n",
    "            #newsmovie,\n",
    "            newsmovie = str(dayNew[\"news_web_movie_uri\"])\n",
    "            \n",
    "            print(id)\n",
