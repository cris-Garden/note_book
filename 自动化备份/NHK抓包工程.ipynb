    "        \n",
    "\n",
    "def save_to_file(file_name, contents):\n",
    "    fh = open(file_name, 'w')\n",
    "    a = fh.write(contents)\n",
    "    fh.close()\n",
    "\n",
    "def ToFile(mp3Text, filePath):\n",
    "    # 文件夹不存在，则创建文件夹\n",
    "    folder = os.path.exists(filePath)\n",
    "    mp3Data = base64.b64decode(mp3Text)\n",
    "    fout = open(filePath, 'wb')\n",
    "    fout.write(mp3Data)\n",
    "    fout.close()\n",
    "\n",
    "# voice表示声音类型\"A\",\"B\",\"C\",\"D\"\n",
    "def getMp3ByNewId(id,voice):\n",
    "    \n",
    "    # MP3文件名\n",
    "    file_name = mp3Path + \"{0}_{1}.mp3\".format(id,voice)\n",
    "    if not os.path.exists(file_name):\n",
    "        new = getNewById(id)\n",
    "        newText = \"\"\n",
    "        for subNew in new[\"body\"]:\n",
    "            newText = newText + subNew[\"origin\"]\n",
    "        #这里配置key\n",
    "        url = \"https://texttospeech.googleapis.com/v1beta1/text:synthesize?"\n",
    "        post = {\n",
    "          \"input\": {\n",
    "            \"text\": newText\n",
    "          },\n",
    "          \"audioConfig\": {\n",
    "            \"audioEncoding\": \"MP3\",\n",
    "            \"pitch\": -0.2,\n",
    "            \"speakingRate\": 0.93,\n",
    "            \"effectsProfileId\": [\n",
    "              \"handset-class-device\"\n",
    "            ]\n",
    "          },\n",
    "          \"voice\": {\n",
    "            \"languageCode\": \"ja-JP\",\n",
    "            \"name\": \"ja-JP-Wavenet-{0}\".format(voice)\n",
    "          }\n",
    "        }\n",
    "        data = json.dumps(post)\n",
    "        #AImp3下载\n",
    "        r = requests.post(url, data=data)\n",
    "        response = json.loads(r.text)\n",
    "        ToFile(response[\"audioContent\"],file_name)\n",
    "        print(file_name + \"保存成功！\")\n",
    "    else:\n",
    "        print(file_name + \"已存在！\")\n",
    "        \n",
    "def getAllAIMp3():\n",
    "    ids = getNewIds()\n",
    "    for id in ids:\n",
    "        getMp3ByNewId(id,\"A\")\n",
    "        getMp3ByNewId(id,\"B\")\n",
    "        getMp3ByNewId(id,\"C\")\n",
    "        getMp3ByNewId(id,\"D\")\n",
    "\n",
    "    \n",
    "            \n",
    "#test\n",
    "print(len(getNewIds()))\n"
   ]
